{
  "display_name": "Apache Toree - Scala2",
  "env": {
    "PYTHONPATH": "/usr/local/spark/python:/usr/local/spark/python/lib/py4j-0.10.4-src.zip",
    "SPARK_HOME": "/usr/local/spark",
    "PYTHON_EXEC": "python",
    "DEFAULT_INTERPRETER": "Scala",
    "__TOREE_SPARK_OPTS__": "",
    "__TOREE_OPTS__": ""
  },
  "argv": [
    "/opt/conda/share/jupyter/kernels/apache_toree_scala/bin/run.sh",
    "--profile",
    "{connection_file}"
  ],
  "language": "scala"
}
